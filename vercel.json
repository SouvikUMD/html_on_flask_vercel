{
  "version": 2,
  "builds": [
      {
          "src": "*.py",
          "use": "@vercel/python"
      }
  ],
  "routes": [
      {
          "src": "/(.*)",
          "dest": "templates/index.html"
      }
  ]
  "rewrites": [
	{ 
	   "src": "/(.*)", 
	   "dest": "/(.*)/app" 
	}
  ]
}